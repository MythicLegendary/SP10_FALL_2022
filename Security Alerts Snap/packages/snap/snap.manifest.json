{
  "version": "0.1.0",
  "description": "An example Snap written in TypeScript.",
  "proposedName": "TypeScript Example Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "G06LwnhXX7wO4pSrdc13D5zbq8sfH5FsCiUDRZRLMwo=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/metamask.svg",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_confirm": {},
    "endowment:network-access": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "checkUnlocked",
            "params": {
              "param1": "parameter"
            }
          }
        },
        {
          "expression": "*/2 * * * *",
          "request": {
            "method": "checkUnlockedAfterTransaction",
            "params": {
              "param1": "parameter"
            }
          }
        }
      ]
    },
    "endowment:transaction-insight": {}
  },
  "manifestVersion": "0.1"
}
